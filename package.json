{
  "name": "react-forms-typescript",
  "version": "1.0.1",
  "description": "React Form, React Form typescript, React Form Javascript",
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "next build",
    "start": "next start -p $PORT",
    "dev": "next",
    "test": "jest",
    "test:watch": "jest --watch",
    "docs": "typedoc",
    "test:coverage": "jest --collectCoverage",
    "build:assets": "rollup -c",
    "postbuild:assets": "node ./utils/copy-assets.js",
    "example": "parcel ./public/index.html"
  },
  "cacheDirectories": [
    "node_modules/",
    ".next/cache/"
  ],
  "keywords": [
    "React Form",
    "React Form Typescript",
    "Reactive React Form"
  ],
  "author": "Khurram Mughal",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "formidable": "^1.2.2",
    "next": "^9.4.4",
    "window": "^4.2.7",
    "@wessberg/rollup-plugin-ts": "^1.2.28",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@types/jest": "^26.0.3",
    "@types/node": "^14.0.14",
    "@types/react": "^16.9.41",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lint-staged": ">=10",
    "parcel-bundler": "1.12.4",
    "prettier": "2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rollup": "2.23.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.6.2",
    "ts-jest": "^26.1.1",
    "typedoc": "0.17.8",
    "typescript": "^3.9.7"
  },
  "lint-staged": {
    "*.{js,css,md,tsx,ts,json}": "prettier --write"
  }
}
