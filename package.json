{
    "name": "react-forms-typescript",
    "version": "1.0.2",
    "description": "React Form, React Form typescript, React Form Javascript",
    "main": "./dist/cjs/index.js",
    "module": "./dist/es/index.js",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "next build",
        "lint": "eslint './src/**/*.{ts,tsx}' --quiet --fix",
        "start": "next start -p $PORT",
        "dev": "next",
        "test": "jest",
        "test:watch": "jest --watch",
        "docs": "typedoc",
        "test:coverage": "jest --collectCoverage",
        "build:assets": "rollup -c",
        "postbuild:assets": "node ./utils/copy-assets.js",
        "example": "parcel ./public/index.html"
    },
    "cacheDirectories": [
        "node_modules/",
        ".next/cache/"
    ],
    "keywords": [
        "React Form",
        "React Form Typescript",
        "Reactive React Form"
    ],
    "author": "Khurram Mughal",
    "license": "MIT",
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "pre-push": "yarn test"
        }
    },
    "lint-staged": {
        "*.{js,ts,tsx}": [
            "eslint --fix"
        ]
    },
    "dependencies": {},
    "devDependencies": {
        "@rollup/plugin-commonjs": "^14.0.0",
        "@rollup/plugin-node-resolve": "^8.4.0",
        "@types/jest": "^26.0.3",
        "@types/node": "^14.0.14",
        "@types/react": "^16.9.41",
        "@typescript-eslint/eslint-plugin": "^3.7.0",
        "@typescript-eslint/parser": "^3.7.0",
        "@wessberg/rollup-plugin-ts": "^1.2.28",
        "eslint": "^7.5.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-react": "^7.20.3",
        "formidable": "^1.2.2",
        "husky": "^4.2.5",
        "jest": "^26.1.0",
        "lint-staged": ">=10",
        "next": "^9.4.4",
        "parcel-bundler": "1.12.4",
        "prettier": "^2.0.5",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "rollup": "2.23.0",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-json": "^4.0.0",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-sourcemaps": "^0.6.2",
        "ts-jest": "^26.1.1",
        "typedoc": "0.17.8",
        "typescript": "^3.9.7",
        "window": "^4.2.7"
    }
}
